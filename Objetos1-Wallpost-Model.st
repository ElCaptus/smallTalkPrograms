Object subclass: #Wallpost	instanceVariableNames: 'meGustas texto destacado'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-Wallpost-Model'!!Wallpost methodsFor: 'initialization' stamp: 'Marcos9098 8/30/2019 20:24'!initialize 	"Inicializar objeto"meGustas:=0.texto:='No asignado'.destacado := false.! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:11'!text: unTexto	"Asigna el texto que viene por parametro"texto:= unTexto .! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:33'!toggleFeatured	"Cambiar el estado de destacado"destacado:= destacado not.! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:26'!dislike	"Se decrementa la variable like"(meGustas >0) ifTrue: [meGustas := meGustas -1].! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:25'!like	"Se incrementa la variable de likes"meGustas := meGustas +1.! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:11'!text 	"Retorna el texto asignado"^texto.! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:23'!isFeatured	"Retorna si esta destacado o no"	^destacado .! !!Wallpost methodsFor: 'public' stamp: 'Marcos9098 8/30/2019 20:25'!likes	"Retorna cantidad total de likes"^meGustas.! !