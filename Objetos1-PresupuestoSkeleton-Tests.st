TestCase subclass: #ItemTest	instanceVariableNames: 'item'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-PresupuestoSkeleton-Tests'!!ItemTest methodsFor: 'running' stamp: 'CompatibleUserName 8/21/2019 14:50:07'!setUp	item := Item new.	item costoUnitario: 25.	item cantidad: 2.	item detalle: 'Porotos'! !!ItemTest methodsFor: 'testing' stamp: 'CompatibleUserName 8/21/2019 14:50:07'!testCosto	self assert: item costo equals: 50.! !!ItemTest methodsFor: 'testing' stamp: 'CompatibleUserName 8/21/2019 14:50:07'!testCostoUnitario	self assert: item costoUnitario equals: 25.! !TestCase subclass: #PresupuestoTest	instanceVariableNames: 'presupuesto'	classVariableNames: ''	poolDictionaries: ''	category: 'Objetos1-PresupuestoSkeleton-Tests'!!PresupuestoTest methodsFor: 'running' stamp: 'CompatibleUserName 8/21/2019 14:50:07'!setUp	presupuesto := Presupuesto new.	presupuesto fecha: Date today.	presupuesto cliente: 'Juan de los Palotes'.	! !!PresupuestoTest methodsFor: 'testing' stamp: 'CompatibleUserName 8/21/2019 14:50:07'!testCalcularTotal	| item |	self assert: presupuesto calcularTotal = 0.	item := Item new.	item		costoUnitario: 10;		cantidad: 1.	presupuesto agregarItem: item.	self assert: presupuesto calcularTotal = 10.	item := Item new.	item		costoUnitario: 10;		cantidad: 1.	presupuesto agregarItem: item.	self assert: presupuesto calcularTotal = 20! !!PresupuestoTest methodsFor: 'testing' stamp: 'CompatibleUserName 8/21/2019 14:50:07'!testInitialize self assert: presupuesto calcularTotal = 0.! !